{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_class": "^1.2",
        "drupal/business_rules": "^1.0@beta",
        "drupal/cancel_account": "^1.1",
        "drupal/captcha": "^1.1",
        "drupal/change_pwd_page": "^1.0",
        "drupal/color_field": "^2.4",
        "drupal/colorbox": "^1.6",
        "drupal/config_filter": "^1.5",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.7",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_install": "^10.4",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/features": "^3.11",
        "drupal/field_formatter": "^1.2",
        "drupal/field_formatter_class": "^1.4",
        "drupal/field_timer": "^1.2",
        "drupal/flag": "^4.0@beta",
        "drupal/fontawesome": "^2.17",
        "drupal/fontawesome_menu_icons": "^1.8",
        "drupal/formblock": "^2.0",
        "drupal/fotorama_gallery": "^2.0",
        "drupal/honeypot": "^2.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/jquery_countdown_timer": "^1.2",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/jquery_ui_draggable": "^1.2",
        "drupal/jquery_ui_slider": "^1.1",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/jstimer": "^1.2",
        "drupal/libraries": "^3.0@alpha",
        "drupal/menu_per_role": "^1.2",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/photoswipe": "^3.0",
        "drupal/plugin": "^2.7",
        "drupal/recaptcha": "^3.0",
        "drupal/registration_link": "^2.3",
        "drupal/responsive_menu": "^4.4",
        "drupal/scheduler": "^1.3",
        "drupal/select_registration_roles": "^1.2",
        "drupal/shs": "1.x-dev",
        "drupal/simple_fb_connect": "^3.1",
        "drupal/simple_sitemap": "^3.7",
        "drupal/social_api": "^2.0",
        "drupal/social_auth_google": "^2.1",
        "drupal/social_login": "^2.89",
        "drupal/taxonomy_manager": "^1.0@beta",
        "drupal/token": "^1.7",
        "drupal/twig_tweak": "^2.6",
        "drupal/twig_vardumper": "^2.2",
        "drupal/upgrade_status": "^2.9",
        "drupal/view_custom_table": "1.x-dev",
        "drupal/views_slideshow": "^4.7",
        "drupal/yearonly": "^9.0",
        "drush/drush": "^10.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "enable-patching": true,
    "patches": {
        "drupal/menu_per_role": {
            "Menu per role, superadmin select for hide register": "https://www.drupal.org/files/issues/2019-12-26/menu_per_role-admin_bypass_option-1.patch"
       },
       
        "drupal/core": {
            "Exposed filters, undefined index min": "https://www.drupal.org/files/issues/2020-06-04/2825860-exposed-filter-notice-38.patch"
       }

     },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
