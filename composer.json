        "drupal/registration_link": "^2.3",
        "drupal/responsive_menu": "^4.4",
        "drupal/scheduler": "^1.3",
        "drupal/select_registration_roles": "^1.2",
        "drupal/shs": "1.x-dev",
        "drupal/simple_fb_connect": "^3.1",
        "drupal/simple_sitemap": "^3.7",
        "drupal/social_api": "^2.0",
        "drupal/social_auth_google": "^2.1",
        "drupal/social_login": "^2.89",
