        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_install": "^10.4",
